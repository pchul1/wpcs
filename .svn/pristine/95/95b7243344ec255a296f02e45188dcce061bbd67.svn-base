.gBox .ptit{display:inline-block; line-height:30px; text-align:center; border-bottom:1px solid #ebebeb;font-weight:600;background:none;padding:0;width:100%;background-color:#f2f2f2; }
.arrbx{height:220px;float:left;margin-left:7px;}


/*-- gallery --------------------------------------*/
.page_protecttechinfo p.locationSta {padding:7px 230px 0 12px;}
.sec_gallery {clear:both; *zoom:1; padding-top:4px; text-align: left;}
.sec_gallery .header {width:100%; padding-top:25px;}
.sec_gallery .photo_tit {padding:0 0 8px 0 !important;}
.sec_gallery .photo_time {float:right; font-size:11px;}
.sec_gallery .photo_img {clear:both;}
.sec_gallery .photo_img p.img img {width:100% !important; height:450px;}
.sec_gallery .photo_con {width:100%; padding:15px 0; line-height:1.4;}
.sec_gallery .photo_btn {width:100%; text-align:right;}

.sec_gallery .photo_list {position:relative; width:100%; background-color:#e9e9e9;}
/* .sec_gallery .photo_list {position:relative;  left:97px; width:795px; background-color:#e9e9e9;} */
.sec_gallery .photo_list p {position:absolute;}
.sec_gallery .photo_list p.btn_prev,
.sec_gallery .photo_list p.btn_next {top:20px; z-index:20;}
.sec_gallery .photo_list p.btn_prev {left:0; padding-left:11px;}
.sec_gallery .photo_list p.btn_next {right:0; padding-right:11px;}

.sec_gallery .photo_list .list {overflow:hidden; width:710px; margin:0 auto; padding:8px 0;}
.sec_gallery .photo_list .list li {float:left; padding:0 2px;}
.sec_gallery .photo_list .list li img {width:66px !important; height:53px !important; vertical-align:top;}
.sec_gallery .photo_list .list li a {float:left; border:3px solid #000;}
.sec_gallery .photo_list .list li a.on,
.sec_gallery .photo_list .list li a:focus,
.sec_gallery .photo_list .list li a:hover {border-color:#2f8bc0;}

.sec_gallery .photo_list .list li img {width:61px !important; height:46px !important;}


/*-- paginate --------------------------------------*/
.paginate {padding-top:15px; text-align:center;}
.paginate li {display:inline; padding:0 3px;}
.paginate li a {display:inline-block; padding:2px;}
.paginate li a:focus,
.paginate li a:hover,
.paginate li a:active {color:#007db6;}
.paginate li em {display:inline-block; padding:2px; font-style:normal; font-weight:bold;}


/*-- layer --------------------------------------*/
#layerFullBgDiv{display:none; position: fixed; left:0; top:0; width:100%; height:100%; background:#000000; opacity:0.4; filter:alpha(opacity=40);z-index:50;}


/*-- tel_spacing --------------------------------------*/
.tel_spacing {line-height: 3;}


/*-- address search --------------------------------------*/
.address {padding:8px;}


/*-- Slick grid null search view --------------------------------------*/
.slickNullSearch {position:absolute;z-index:3;left:495px;top:28px;}



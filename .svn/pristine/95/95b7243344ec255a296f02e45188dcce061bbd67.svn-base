/*-- tabmenu --------------------------------------*/

.tab_container {padding-top:5px;}

.tab_container2 {padding-top:110px;}

ul.tabs_top {margin: 5px 0 15px 0 ; padding: 0; list-style: none;width: 100%; border-bottom:2px solid #228FCA; display:inline-block;}
ul.tabs_top li {
	cursor:pointer;
	float: left;
	margin: 10px 0 -2px 0;
	padding: 0 2px;
	height: 30px;
	line-height: 30px;
	width: 157px;
	overflow: hidden;
	position: relative;
	border-top:2px solid #FFFFFF;
	border-bottom:2px solid #228FCA;
	border-radius: 8px 8px 0 0;
	-webkit-border-radius: 8px 8px 0 0;
	-moz-border-radius: 8px 8px 0 0;
	background: #8BBDFF;
	background: -moz-linear-gradient(top, #8BBDFF 0%, rgba(86, 153, 216, 1) 100%);
	background:-webkit-linear-gradient(top, #8BBDFF 0%, rgba(86, 153, 216, 1) 100%);
	background:-o-linear-gradient(top, #8BBDFF 0%, rgba(86, 153, 216, 1) 100%);
	background:-ms-linear-gradient(top, #8BBDFF 0%, rgba(86, 153, 216, 1) 100%);
	background:linear-gradient(to bottom, #8BBDFF 0%, rgba(86, 153, 216, 1) 100%);
}
ul.tabs_top li a {font-family:dotum; color:#FFFFFF; font-weight:bold;}
ul.tabs_top li:hover, ul.tabs_top li.on {background:#FFFFFF; padding:0; border-top:2px solid #228FCA; border-left:2px solid #228FCA; border-right:2px solid #228FCA; border-bottom:2px solid #FFFFFF;}
ul.tabs_top li:hover a, ul.tabs_top li.on a {color: #147CBE;}
ul.tabs_top li.li_space {width:2px; background:#FFFFFF; border:0; padding:0; border-top:2px solid #FFFFFF; border-bottom:2px solid #228FCA;}

ul.tabs {margin: 5px 0 20px 0 ; padding: 0; list-style: none;width: 100%; display:inline-block;}
ul.tabs li {float: left;margin: 10px 0 -2px 0; padding: 0 2px; height: 30px; line-height: 30px; width: 157px; overflow: hidden;position: relative; background:#EAEAEA; border-top:2px solid #FFFFFF; border-bottom:2px solid #A6A6A6; border-radius: 8px 8px 0 0; -webkit-border-radius: 8px 8px 0 0; -moz-border-radius: 8px 8px 0 0;}
ul.tabs li a {font-family:dotum; color:#767676; font-weight:bold;}
ul.tabs li:hover, ul.tabs li.on {background:#FFFFFF; padding:0; border-top:2px solid #A6A6A6; border-left:2px solid #A6A6A6; border-right:2px solid #A6A6A6; border-bottom:2px solid #FFFFFF;}
/* ul.tabs li:hover a, ul.tabs li.on a {color: #147CBE;} */
ul.tabs li.li_space {width:4px; background:#FFFFFF; border:0; padding:0; border-top:2px solid #FFFFFF; border-bottom:2px solid #A6A6A6;}



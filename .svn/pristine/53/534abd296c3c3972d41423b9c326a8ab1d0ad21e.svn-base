/*-- gallery --------------------------------------*/
.page_protecttechinfo p.locationSta {padding:7px 230px 0 12px;}
.sec_gallery {clear:both; *zoom:1; padding-top:4px;}
.sec_gallery .header {width:756px; padding-top:25px; padding-bottom: 30px;}
.sec_gallery .photo_tit {padding:0 0 8px 0 !important;}
.sec_gallery .photo_time {float:right; font-size:11px;}
.sec_gallery .photo_img {clear:both;}
.sec_gallery .photo_img p.img img {width:756px !important; height:450px;}
.sec_gallery .photo_con {width:756px; padding:15px 0; line-height:1.4;}
.sec_gallery .photo_btn {width:756px; text-align:right;}

.sec_gallery .photo_list {position:relative; _left:97px; width:756px; background-color:#e9e9e9;}
.sec_gallery .photo_list p {position:absolute;}
.sec_gallery .photo_list p.btn_prev,
.sec_gallery .photo_list p.btn_next {top:20px; z-index:20;}
.sec_gallery .photo_list p.btn_prev {left:0; padding-left:11px;}
.sec_gallery .photo_list p.btn_next {right:0; padding-right:11px;}

.sec_gallery .photo_list .list {overflow:hidden; width:670px; margin:0 auto; padding:8px 0;}
.sec_gallery .photo_list .list li {float:left; padding:0 2px;}
.sec_gallery .photo_list .list li img {width:66px !important; height:53px !important; vertical-align:top;}
.sec_gallery .photo_list .list li a {float:left; border:3px solid #e9e9e9;}
.sec_gallery .photo_list .list li a.on,
.sec_gallery .photo_list .list li a:focus,
.sec_gallery .photo_list .list li a:hover {border-color:#2f8bc0;}

.sec_gallery .photo_list .list li img {width:61px !important; height:46px !important;}

.right_line { border-right: 1px solid #d4d4d4; }
.thead_top_bg { background-image: url(/images/index/sub/table_top_bg.gif); background-repeat: repeat-x; border-top: 1px solid #d4d4d4; }
.thead_top_bg64 { _background-image: url(/images/index/sub/table_top_bg_64.gif); background-repeat: repeat-x; }
.second_line_bg { _background-image: url(/images/index/sub/table_top_bg_second.gif); background-repeat: repeat-x; }


/*운영체계도*/
.system { width: 756px; height: 839px; position: relative; background-image: url(/images/index/sub/system_bg.gif); background-repeat: no-repeat; }
.sys_info { position: relative; clear: both; }
.sys_info li { width: auto; float: left; }
.sys_info li span { width: 450px; font-size: 15px; _letter-spacing: -2px; color: #000000; line-height: 24px; }


/*매뉴얼*/
.manual { width: 756px; height: 16px; }
.m_down { width: 174px; height: 16px; float: right; }
.exp_2 { font-size: 13px; line-height: 22px; clear: both; padding-left: 20px; margin-bottom: 50px; }

.jon_scroll_box {border:1px solid #c9c9c9; padding:15px; font-size:11px; color:#a0a0a0; height:280px; overflow-y:scroll;}

.sitemap_dl1 {float:left; width:169px; margin-right:12px; height:220px; line-height:2; border:1px solid #b8d1e8}
.sitemap_dl2 {float:left; width:169px; margin-right:12px; height:220px;  margin-top:20px; line-height:2; border:1px solid #b8d1e8}
.sitemap_dt {height:31px; text-align:center; border-bottom:1px solid #b8d1e8; background:#f9f9f9; color:#4e83b4; font-size:15px;}
.sitemap_dd {padding-left:13px; margin-top:11px;}
.sitemap_dd1 {padding-left:13px;}



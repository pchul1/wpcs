<script language="javascript">
	var isClicked = 'pRadial';

	function pSwapOver(id)
	{
		if(isClicked !=id)
		{
			var objP = document.getElementById(id);
			objP.style.background = '#E2E6EC';
			objP.style.cursor = 'pointer'
		}
	}

	function pSwapOut(id)
	{
		if(isClicked !=id)
		{
			var objP = document.getElementById(id);
			objP.style.background = '#CED4DC';
			objP.style.cursor = 'default';
		}
	}
	
	function pClick(id)
	{
		if(isClicked != null)
		{
			var objIsClicked = document.getElementById(isClicked);
			objIsClicked.style.background = '#CED4DC';
		}

		var objP = document.getElementById(id);
		objP.style.background = 'white';
		isClicked = id;
	}
</script>


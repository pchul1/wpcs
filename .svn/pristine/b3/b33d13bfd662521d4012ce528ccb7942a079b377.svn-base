<body onLoad="javascript: parent.main.location='radial.htm'";>
	<p class="header">Samples</p>
	<p style="background:white;" class="button" id="pRadial" onMouseOver="pSwapOver(id);" onMouseOut="pSwapOut(id);" onClick="parent.main.location='radial.htm'; pClick(id);">Radial Gauges</p>
	<p class="button" id="pLinear" onMouseOver="pSwapOver(id);" onMouseOut="pSwapOut(id);" onClick="parent.main.location='linear.htm'; pClick(id);">Linear Gauges</p>
	<p class="button" id="pDigital" onMouseOver="pSwapOver(id);" onMouseOut="pSwapOut(id);" onClick="parent.main.location='DigitalPanel/DigitalPanel.jsp'; pClick(id);">Digital Panels</p>
	
	<p class="button" id="pRealTime" onMouseOver="pSwapOver(id);" onMouseOut="pSwapOut(id);" onClick="parent.main.location='RealTime/RealTime.htm'; pClick(id);">Real Time</p>
	<p class="button" id="pAjax" onMouseOver="pSwapOver(id);" onMouseOut="pSwapOut(id);" onClick="parent.main.location='ajax.htm'; pClick(id);">Ajax</p>

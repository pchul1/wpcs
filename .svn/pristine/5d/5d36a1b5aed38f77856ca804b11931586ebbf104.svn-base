<script language="javascript">
	
	var isClicked = 'sHistograms';

	function sSwapOver(id)
	{
		if(isClicked !=id)
		{
			var objP = document.getElementById(id);
			objP.className = 'buttonOver';
			objP.style.cursor = 'pointer';
		}
	}

	function sSwapOut(id)
	{
		if(isClicked !=id)
		{
			var objP = document.getElementById(id);
			objP.className = 'button';
			objP.style.cursor = 'default';
		}
	}
	
	function sClick(id)
	{
		if(isClicked != null)
		{
			var objIsClicked = document.getElementById(isClicked);
			objIsClicked.className = 'button';
		}

		var objP = document.getElementById(id);
		objP.className = 'buttonOn';
		isClicked = id;
	}
</script>
</head>
<body>	
	<div id="PanelTitle" style="Height:31px; font-size:9pt; font-family:Arial; 
		text-align:justify; Width:698px"> The Chart FX Extensions Pack provides 
     	a full set of statistical functions like SPC, 
     	Linear Regression, ANOVA and Capability Analysis among others. To test, run the application 
     	and click on the tabs below for the different statistical studies provided.
   	</div>
   	&nbsp;&nbsp;
	<table width="696" height="25" border="0" cellpadding="0" cellspacing="0">
		<tr>
			<td nowrap class="buttonOn" id="sHistograms" onClick="sClick(id); parent.subMain.location='Charts/Histogram.jsp';" onMouseOver="sSwapOver(id);" onMouseOut="sSwapOut(id);">Histogram</td>
			<td class="underLine">&nbsp;</td>
			<td nowrap class="button" id="sSPC" onClick="sClick(id); parent.subMain.location='Charts/SPC.jsp';" onMouseOver="sSwapOver(id);" onMouseOut="sSwapOut(id);">SPC (Six Sigma)</td>
			<td class="underLine">&nbsp;</td>
			<td nowrap class="button" id="sBoxPlots" onClick="sClick(id); parent.subMain.location='Charts/BoxPlots.jsp';" onMouseOver="sSwapOver(id);" onMouseOut="sSwapOut(id);">Sections</td>
			<td class="underLine">&nbsp;</td>
			<td nowrap class="button" id="sLinearRegression" onClick="sClick(id); parent.subMain.location='Charts/LinearRegression.jsp';" onMouseOver="sSwapOver(id);" onMouseOut="sSwapOut(id);">Linear Regression</td>
			<td class="underLine">&nbsp;</td>
			<td nowrap class="button" id="sANOVA" onClick="sClick(id); parent.subMain.location='Charts/ANOVA.jsp';" onMouseOver="sSwapOver(id);" onMouseOut="sSwapOut(id);">ANOVA</td>
			<td class="underLine">&nbsp;</td>
			<td nowrap class="button" id="sPolyRegression" onClick="sClick(id); parent.subMain.location='Charts/PolyRegression.jsp';" onMouseOver="sSwapOver(id);" onMouseOut="sSwapOut(id);">Polynomial Regression</td>

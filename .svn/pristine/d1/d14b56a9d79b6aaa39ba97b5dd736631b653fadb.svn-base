<body>
<table width="696" height="25" border="0" cellpadding="0" cellspacing="0">
  <tr>
    <td nowrap class="buttonOn" id="sScales" onClick="sClick(id); parent.subMain.location='LinearGauges/MultipleScales.jsp';" onMouseOver="sSwapOver(id);" onMouseOut="sSwapOut(id);">Multiple
        Scales</td>
    <td class="underLine">&nbsp;</td>
    <td nowrap class="button" id="sImages" onClick="sClick(id); parent.subMain.location='LinearGauges/Images.jsp';" onMouseOver="sSwapOver(id);" onMouseOut="sSwapOut(id);">Images</td>
    <td class="underLine">&nbsp;</td>
    <td nowrap class="button" id="sSections" onClick="sClick(id); parent.subMain.location='LinearGauges/Sections.jsp';" onMouseOver="sSwapOver(id);" onMouseOut="sSwapOut(id);">Sections</td>
    <td class="underLine">&nbsp;</td>
    <td nowrap class="button" id="sIndicators" onClick="sClick(id); parent.subMain.location='LinearGauges/MultipleIndicators.jsp';" onMouseOver="sSwapOver(id);" onMouseOut="sSwapOut(id);">Multiple
      Indicators </td>
    <td class="underLine">&nbsp;</td>
    <td nowrap class="button" id="sStripes" onClick="sClick(id); parent.subMain.location='LinearGauges/Stripes.jsp';" onMouseOver="sSwapOver(id);" onMouseOut="sSwapOut(id);">Stripes</td>
    <td class="underLine">&nbsp;</td>
    <td nowrap class="button" id="sCombo" onClick="sClick(id); parent.subMain.location='LinearGauges/Combination.jsp';" onMouseOver="sSwapOver(id);" onMouseOut="sSwapOut(id);">Combination</td>

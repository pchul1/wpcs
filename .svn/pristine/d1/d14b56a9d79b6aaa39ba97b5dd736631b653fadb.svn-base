<script language="javascript">
	
	var isClicked = 'sScales';

	function sSwapOver(id)
	{
		if(isClicked !=id)
		{
			var objP = document.getElementById(id);
			objP.className = 'buttonOver';
			objP.style.cursor = 'pointer';
		}
	}

	function sSwapOut(id)
	{
		if(isClicked !=id)
		{
			var objP = document.getElementById(id);
			objP.className = 'button';
			objP.style.cursor = 'default';
		}
	}
	
	function sClick(id)
	{
		if(isClicked != null)
		{
			var objIsClicked = document.getElementById(isClicked);
			objIsClicked.className = 'button';
		}

		var objP = document.getElementById(id);
		objP.className = 'buttonOn';
		isClicked = id;
	}
</script>

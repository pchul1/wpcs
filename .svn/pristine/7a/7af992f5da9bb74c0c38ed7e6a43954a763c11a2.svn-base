function tab(idx){
	for(i = 1; i <= 2 ; i++ ){
		document.getElementById('tabimg'+i).src= "/images/new/water_tab0"+ i +".gif";
		document.getElementById('content'+i).className= "tabcont0" + i + " hide";
		
	}
	document.getElementById('tabimg'+idx).src= "/images/new/water_tab0"+ idx +"_on.gif";
	document.getElementById('content'+idx).className= "tabcont0" + idx + " show";
}



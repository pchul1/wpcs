<html>
<head>
    <title>Ajax update</title>
    <meta content="Microsoft Visual Studio .NET 7.1" name="GENERATOR">
    <meta content="C#" name="CODE_LANGUAGE">
    <meta content="JavaScript" name="vs_defaultClientScript">
    <meta content="http://schemas.microsoft.com/intellisense/ie5" name="vs_targetSchema">
    <link href="../cfxgauges.css" type="text/css" rel="stylesheet">

    <script type="text/javascript" src="../exampleutils.js"></script>

    <script type="text/javascript">

		
function imageLoaded(src)
{
	var sourceGauge = document.getElementById('gauge2');
	var targetGauge = document.getElementById('gauge1');
	targetGauge.src = sourceGauge.src;
}


function RefreshGauge(){

	//alert(sBorder);
	//var swappableSection = document.getElementById('gauge2');
	//swappableSection.src = "MultipleScalesSource.jsp?b="+sBorder;
	var sParams = "";
	sParams = "s=" + document.getElementById('Text').value;
	//alert(sParams);
			var img = document.getElementById('gauge1');
	            	var src = img.src
			src = 'DigitalPanelSource.jsp?' + sParams;

			var swappableSection = document.getElementById('gauge2');
			swappableSection.src = src;
}
function LoadGauge(){
mygauge= document.getElementById('gauge1');
//var sParams = "";
//	sParams = "s=" + document.getElementById('Text').value;
mygauge.src= 'DigitalPanelSource.jsp';
}

    </script>

</head>
<body onload="LoadGauge()">
    <form id="Form1" method="post" runat="server">
        <table class="bodyBorder" cellspacing="0" cellpadding="0" width="696" border="0">
            <tr>
                <td>
                    <p>
                        <br>
                        This sample uses AJAX to update a DigitalPanel to reflect the contents of a text
                        box. To view the client-side script simply right-click on the page and select "View
                        Source".</p>
                    <p>
                        Enter the text (up to 25 characters):&nbsp;
                        <input type="text" id="Text" size="25" onkeyup="javascript:RefreshGauge();" maxlength="25"></p>
                    <p align="center">
                        <img id="gauge1" height="70" src="DigitalPanelSource.jsp" width="500" border="0"></p>
                    <p align="center">
                        &nbsp;</p>
                </td>
            </tr>
            <tr>
                <td>
                </td>
            </tr>
        </table>
        <img src="sample.gif" onload="imageLoaded(this.id)" id='gauge2' style="display: none">
    </form>
</body>
</html>

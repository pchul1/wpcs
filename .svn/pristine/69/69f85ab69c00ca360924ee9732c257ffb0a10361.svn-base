.over_menu { width:168px; height:25px; background-image: url(../images/left_menu_over_bg.gif); background-repeat: no-repeat; padding-left: 4px; font-size: 14px; font-weight: bold; color: #FFFFFF; vertical-align: middle; padding-top:5px; letter-spacing:-1px; }

.low_menu { width:152px; padding-left:20px; font-size: 12px; color: #626262; line-height:22px; margin-top:10px; margin-bottom:10px; letter-spacing:-1px; }

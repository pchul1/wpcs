.low_menu a:hover { font-weight: bold; color: #e27907; text-decoration: none;}

.off_menu { width:172px; height:16px; border-top-width: 1px; border-top-style: solid; border-top-color: #e3e3e3; font-size: 14px; font-weight: bold; color: #4b4b4b; letter-spacing:-1px; padding-top:8px; line-height:30px; margin-bottom:15px; text-indent: 4px; }
.off_menu a:link, a:visited, a:active { color: #4b4b4b; }
.off_menu a:hover { background-image: url(../images/left_menu_over_bg.gif); background-repeat: no-repeat; color:#FFFFFF; display:block; height:30px; line-height:30px;  }


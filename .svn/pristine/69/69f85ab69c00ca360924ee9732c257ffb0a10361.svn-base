/**tap**/
#tap_warp { width:1026px; height:40px; margin-top:26px; }
.tap { width:143px; height:40px; background-image: url(../images/tap_menu_off.gif); background-repeat: no-repeat; text-align:center; font-size: 12px; font-weight: bold; color: #333333; margin-top:8px; line-height:33px; margin-right:4px; float:left; letter-spacing:-1px  }
.tap a:link, a:visited, a:active { color: #333333; }
.tap a:hover { display:block; height:40px; color:#FFFFFF; line-height:33px; background-image: url(../images/tap_menu_over.gif); background-repeat: no-repeat; }

